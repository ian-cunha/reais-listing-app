{
  "expo": {
    "name": "REAIs Listing",
    "slug": "reais-listing",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleDevelopmentRegion": "pt-BR",
        "CFBundleLocalizations": [
          "en",
          "pt-BR"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSLocationWhenInUseUsageDescription": "O REAIs Listing utiliza sua localização para exibir imóveis próximos a você."
      },
      "buildNumber": "0.0.1",
      "bundleIdentifier": "com.realestateaiplanner.reaislisting"
    },
    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.realestateaiplanner.reaislisting"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "131e6dae-1697-4da3-a232-6eef6510d5b8"
      }
    },
    "owner": "realestateaiplanner"
  }
}